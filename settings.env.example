# WikiWiki API Configuration
WIKIWIKI_ID=yourWikiwikiIdName
WIKIWIKI_URL_BASE=https://wikiwiki.jp/
WIKIWIKI_API_KEY_ID=yourApiKey
WIKIWIKI_API_SECRET=yourApiSecret

# Discord Webhook Configuration
DISCORD_WEBHOOK_URL=yourWebhookURL

# Monitored Pages (comma-separated)
WIKIWIKI_PAGE_NAMES=yourMonitoredPages1,yourMonitoredPages2

# Auto-Track Patterns (comma-separated regex patterns)
WIKIWIKI_AUTO_TRACK_PATTERNS=yourAutoMonitoredPagesPattern1,yourAutoMonitoredPagesPattern2

# Display Full-diff Preview pages
WIKIWIKI_DIFF_FULL_PAGES = yourDiffFullPages1,yourDiffFullPages2

# Monitor RecentCreated page
WIKIWIKI_MONITOR_RECENT_CREATED=true

# State File Path
STATE_PATH=state.json

# Snapshots Dir Path
SNAPSHOTS_DIR_PATH=.snapshots